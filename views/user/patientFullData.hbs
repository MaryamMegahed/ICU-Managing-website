<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Patoent Full Data</title>
    <link rel="stylesheet" href="../CSS/all.min.css" />
    <link rel="stylesheet" href="../CSS/style.css" />
    <link rel="stylesheet" href="../CSS/main.css" />
    <link rel="stylesheet" href="../CSS/admin.css" />
    <link rel="stylesheet" href="../CSS/patientFullData.css" />
    <link rel="stylesheet" href="../CSS/Normalize.css" />
    <link rel="stylesheet" href="../CSS/bootstrap-grid.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="container">
      <div class="header">
        <div class="logo home-btn">
          <p class="home-btn">New Life</p>
          <h2 class="home-btn">ICU</h2>
        </div>
        <div class="links">
          <ul class="links">
          <li><a href="http://localhost:8000/aboutus">Department</a></li>
            <li><a href="http://localhost:8000/aboutus#ayhaga">About US</a></li>
          </ul>
        </div>
      </div>
    </div>
    <div class="data-report">
      <div class="patients doctor-patients messages">
        <div class="contains">
          <div class="sp-header patients-refresh complaints">Patient Data</div>
          <div class="pa-data row">
            <div class="data-name data col-8 right">
              <h3>Name</h3>
              <h2>{{patient.pName}}</h2>
            </div>
            <div class="data data-age col-2 right left">
              <h3>Age</h3>
              <h2>{{age}}</h2>
            </div>
            <div class="data data-gender col-2 left">
              <h3>Gender</h3>
              <h2>{{patient.pGender}}</h2>
            </div>
            <div class="data data-adress col-6 right">
              <h3>Address</h3>
              <h2>{{patient.pAddress}}</h2>
            </div>
            <div class="data data-phone col-6 left">
              <h3>Phone</h3>
              <h2>{{patient.pfirstNum}}</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="data-report">
      <div class="patients doctor-patients messages">
        <div class="contains">
          <div class="sp-header patients-refresh complaints">Hospital Data</div>
          <div class="pa-data row">
            <div class="data-name data col-5 right">
              <h3>Department</h3>
              <h2>{{patientReport.vDepartment}}</h2>
            </div>
            <div class="data data-age col-2 right left">
              <h3>Room No</h3>
              <h2>{{patient.roomNum}}</h2>
            </div>
            <div class="data data-gender col-5 left">
              <h3>Arrival Date</h3>
              <h2>{{arrDate}}</h2>
            </div>
            <div class="data data-adress col-6 right">
              <h3>Doctor Name</h3>
              <h2>{{doc.empName}}</h2>
            </div>
            <div class="data data-phone col-6 left">
              <h3>Nurse Name</h3>
              <h2>{{nurse.empName}}</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="data-report">
      <div class="patients doctor-patients messages">
        <div class="contains">
          <div class="sp-header patients-refresh complaints"id="ayhaga">Medical Data</div>
          <div class="pa-data row">
            <div class="data-name data col-5 right">
              <h3>Diagnosis</h3>
              <h2>{{patientReport.report.diagnosis}}</h2>
            </div>
            <div class="data data-age col-5 right left">
              <h3>Medicines</h3>
              <h2>{{patientReport.report.midicines}}</h2>
            </div>
            <div class="data data-gender col-2 left">
              <h3>Blood Tybe</h3>
              <h2>{{patient.bloodType}}</h2>
            </div>
            <div class="data data-adress col-4 right">
              <h3>Last Blood Presure</h3>
              <h2>{{diags.bloodPressure}}</h2>
            </div>
            <div class="data data-phone col-4 left right">
              <h3>Last Blood Glocuse</h3>
              <h2>{{diags.bloodGlucose}}</h2>
            </div>
            <div class="data data-phone col-4 left">
              <h3>Progrss</h3>
              <h2>{{patient.progress}}%</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <form action="/" method="get">
      <button class="button">Back</button>
    </form>
    <div class="last"></div>
  </body>
  <script src="../JS/all.min.js"></script>
  <script src="../JS/main.js"></script>
  <script src="../JS/bootstrap.bundle.min.js"></script>
</html>